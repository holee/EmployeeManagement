{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "admin-lte@3.2.0",
      "destination": "wwwroot/lte/"
    },
    {
      "library": "font-awesome@6.3.0",
      "destination": "wwwroot/fonts/"
    },
    {
      "library": "jquery@3.6.3",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    }
  ]
}